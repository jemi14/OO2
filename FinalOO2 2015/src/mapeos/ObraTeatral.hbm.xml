<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="datos.ObraTeatral" table="obraTeatral">

		<id column="idObraTeatral" name="idObraTeatral">
			<generator class="identity" />
		</id>

		<property column="obraTeatral" name="obraTeatral" type="string" />
		
		<set name="funciones" table="funcion" order-by="idFuncion asc"
			inverse="true" lazy="true" fetch="select">
			<key column="idObraTeatral" not-null="true" />
			<one-to-many class="datos.Funcion" />
		</set>

	</class>
</hibernate-mapping>